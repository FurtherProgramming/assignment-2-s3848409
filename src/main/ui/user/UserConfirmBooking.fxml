<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="366.0" xmlns="http://javafx.com/javafx/8.0.202-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.user.ConfirmBookingController">
   <Label layoutX="123.0" layoutY="28.0" text="Confirm Booking">
      <font>
         <Font name="System Bold" size="15.0"/>
      </font>
   </Label>
   <Label alignment="CENTER_RIGHT" layoutX="127.0" layoutY="89.0" text="Owner:">
      <font>
         <Font size="15.0"/>
      </font>
   </Label>
   <Label fx:id="lblOwner" layoutX="199.0" layoutY="89.0" prefHeight="21.0" prefWidth="146.0" text="&lt;Owner Name&gt;">
      <font>
         <Font size="15.0"/>
      </font>
   </Label>
   <Label alignment="CENTER_RIGHT" layoutX="81.0" layoutY="140.0" text="Booking date:">
      <font>
         <Font size="15.0"/>
      </font>
   </Label>
   <Label alignment="CENTER_RIGHT" layoutX="140.0" layoutY="172.0" text="Seat:">
      <font>
         <Font size="15.0"/>
      </font>
   </Label>
   <Label fx:id="lblSeat" layoutX="199.0" layoutY="172.0" prefHeight="21.0" prefWidth="146.0"
          text="&lt;Seat number&gt;">
      <font>
         <Font size="15.0"/>
      </font>
   </Label>
   <Label fx:id="lblDate" layoutX="199.0" layoutY="140.0" prefHeight="21.0" prefWidth="146.0" text="&lt;Date&gt;">
      <font>
         <Font size="15.0"/>
      </font>
   </Label>
   <Button fx:id="btnBack" layoutX="67.0" layoutY="239.0" mnemonicParsing="false" onAction="#BackToUserBooking"
           prefHeight="40.0" prefWidth="100.0"
           style="-fx-background-radius: 8; -fx-background-color: #0f73fb; -fx-font-weight: bold;" text="Back"
           textFill="WHITE"/>
   <Button fx:id="btnConfirmBooking" layoutX="198.0" layoutY="239.0" mnemonicParsing="false" onAction="#ConfirmBooking"
           prefHeight="40.0" prefWidth="100.0"
           style="-fx-background-radius: 8; -fx-background-color: #11cb53; -fx-font-weight: bold;" text="Confirm"
           textFill="WHITE"/>
</AnchorPane>
